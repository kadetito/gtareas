<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.gtareascordova" version="1.6.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>gTareas</name>
    <description>
        Gesti√≥n de tareas
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Rafa P
    </author>
    <content src="index.html" />
    <config-file overwrite="true" parent="NSCameraUsageDescription" platform="ios" target="*-Info.plist">
        <string>Allow the app to use your camera</string>
    </config-file>
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="android-minSdkVersion" value="9" />
    <preference name="Fullscreen" value="true" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="KeepRunning" value="true" />
</widget>
