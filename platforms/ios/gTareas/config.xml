<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.gtareascordova" version="1.6.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="Console">
        <param name="ios-package" value="CDVLogger" />
        <param name="onload" value="true" />
    </feature>
    <feature name="HandleOpenUrl">
        <param name="ios-package" value="CDVHandleOpenURL" />
        <param name="onload" value="true" />
    </feature>
    <feature name="IntentAndNavigationFilter">
        <param name="ios-package" value="CDVIntentAndNavigationFilter" />
        <param name="onload" value="true" />
    </feature>
    <feature name="GestureHandler">
        <param name="ios-package" value="CDVGestureHandler" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Camera">
        <param name="ios-package" value="CDVCamera" />
    </feature>
    <feature name="CameraPreview">
        <param name="ios-package" onload="true" value="CameraPreview" />
    </feature>
    <feature name="StatusBar">
        <param name="ios-package" value="CDVStatusBar" />
        <param name="onload" value="true" />
    </feature>
    <name>gTareas</name>
    <description>
        Gesti√≥n de tareas
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Rafa P
    </author>
    <content src="index.html" />
    <gap:plugin name="cordova-plugin-badge" source="npm" version="0.8.8" />
    <config-file overwrite="true" parent="NSCameraUsageDescription" platform="ios" target="*-Info.plist">
        <string>Allow the app to use your camera</string>
    </config-file>
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
    <icon height="180" src="www/res/icon/apple-icon-180x180.png" width="180" />
    <icon height="60" src="www/res/icon/apple-icon-60x60.png" width="60" />
    <icon height="120" src="www/res/icon/apple-icon-120x120.png" width="120" />
    <icon height="76" src="www/res/icon/apple-icon-76x76.png" width="76" />
    <icon height="152" src="www/res/icon/apple-icon-152x152.png" width="152" />
    <icon height="80" src="www/res/icon/cordova_bb_80.png" width="80" />
    <icon height="57" src="www/res/icon/apple-icon-57x57.png" width="57" />
    <icon height="114" src="www/res/icon/cordova_ios_114.png" width="114" />
    <icon height="72" src="www/res/icon/cordova_ios_72.png" width="72" />
    <icon height="144" src="www/res/icon/cordova_ios_144.png" width="144" />
    <splash height="480" src="www/res/screen/iphone_portrait.png" width="320" />
    <splash height="1024" src="www/res/screen/ipad_portrait.png" width="768" />
    <splash height="2048" src="www/res/screen/ipad_retina_landscape.png" width="1536" />
    <splash height="768" src="www/res/screen/ipad_landscape.png" width="1024" />
    <splash height="1536" src="www/res/screen/ipad_retina_portrait.png" width="2048" />
    <splash height="1136" src="www/res/screen/iphone_retina_portrait.png" width="640" />
    <splash height="1334" src="www/res/screen/ipad_portrait.png" width="750" />
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="SuppressesLongPressGesture" value="false" />
    <preference name="Suppresses3DTouchGesture" value="false" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <preference name="CameraUsesGeolocation" value="false" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarStyle" value="lightcontent" />
    <preference name="android-minSdkVersion" value="9" />
    <preference name="Fullscreen" value="true" />
    <preference name="KeepRunning" value="true" />
</widget>
